<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>$id$</id>
        <version>$version$</version>
        <authors>$authors$</authors>
        <owners>$authors$</owners>
        <license type="file">$licenseFile$</license>
        <requireLicenseAcceptance>$requireLicenseAccept$</requireLicenseAcceptance>
        <projectUrl>http://github.com/mwpowellhtx/Xunit.Fluently</projectUrl>
        <description>$description$</description>
        <copyright>$copyright$</copyright>
        <tags>$tags$</tags>
        <dependencies>
            <group targetFramework=".NETStandard1.1">
                <dependency id="NETStandard.Library" version="1.6.1" exclude="Build,Analyzers" />
                <dependency id="xunit.core" version="2.4.1" exclude="Build,Analyzers" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="..\..\$licenseFile$" target="." />
    </files>
</package>